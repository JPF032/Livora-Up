# Configuration PostgreSQL (à remplacer par vos valeurs réelles)
DATABASE_URL=postgresql://username:password@localhost:5432/livoradb

# Configuration Clarifai pour l'IA (à remplacer par vos valeurs réelles)
CLARIFAI_PAT=your_personal_access_token
CLARIFAI_LLM_MODEL_ID=your_llm_model_id
CLARIFAI_USER_ID=your_user_id
CLARIFAI_APP_ID=your_app_id

# Configuration Nutritionix pour l'analyse nutritionnelle (à obtenir sur developer.nutritionix.com)
NUTRITIONIX_APP_ID=your_nutritionix_app_id
NUTRITIONIX_API_KEY=your_nutritionix_api_key

# Configuration Firebase Admin (à remplacer par vos valeurs réelles)
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nVotre clé privée ici\n-----END PRIVATE KEY-----"
FIREBASE_CLIENT_EMAIL=your-service-account-email@example.com

# Configuration du serveur
PORT=3000
NODE_ENV=development
