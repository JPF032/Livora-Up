{
  "name": "livoraupexpo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "dev": "node ./scripts/start-dev.js",
    "verify": "node ./scripts/verify-migration.js",
    "clear-cache": "expo start --clear",
    "// SCRIPTS MULTI-ENVIRONNEMENT": "=======================",
    "start:dev": "npx cross-env APP_ENV=development expo start --clear",
    "start:prod": "npx cross-env APP_ENV=production expo start --clear",
    "build:android:dev": "npx cross-env APP_ENV=development eas build --platform android --profile development",
    "build:android:prod": "npx cross-env APP_ENV=production eas build --platform android --profile production",
    "build:ios:dev": "npx cross-env APP_ENV=development eas build --platform ios --profile development",
    "build:ios:prod": "npx cross-env APP_ENV=production eas build --platform ios --profile production",
    "publish:dev": "npx cross-env APP_ENV=development eas update --branch development",
    "publish:prod": "npx cross-env APP_ENV=production eas update --branch production"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@google-cloud/speech": "^7.0.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-community/slider": "^4.5.6",
    "@react-native-picker/picker": "^2.11.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "axios": "^1.6.2",
    "expo": "~53.0.9",
    "expo-app-loading": "^2.1.1",
    "expo-audio": "^0.4.5",
    "expo-av": "^15.1.4",
    "expo-blur": "~14.1.4",
    "expo-camera": "~16.1.6",
    "expo-constants": "~17.1.6",
    "expo-file-system": "^18.1.10",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.1.7",
    "expo-image-manipulator": "~13.1.6",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.0.6",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.7",
    "expo-video": "^2.1.9",
    "expo-web-browser": "~14.1.6",
    "firebase": "^11.7.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
